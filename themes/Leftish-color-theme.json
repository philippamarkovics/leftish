	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "bold italic",
      	"foreground": "#000"
			}
		},
		{
			"name": "Regexp",
			"scope": [
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#3e8281"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords, Characters",
			"scope": [
				"constant.character",
				"constant.keyword",
				"constant"
			],
			"settings": {
				"foreground": "#3e8281"
			}
		},
		{
			"name": "Control flow keywords, Property names",
			"scope": [
				"keyword.control.flow",
				"support.type.property-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3e8281"
			}
		},
		{
			"name": "HTML tags",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#3e8281"
			}
		},
		{
			"name": "Booleans, Bindings",
			"scope": [
				"constant.language.boolean",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#333333"
			}
		},
		{
			"name": "Global definitions, function definitions",
			"scope": [
				"entity.global",
				"meta.definition.function"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#9E9C9C"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	],
}
